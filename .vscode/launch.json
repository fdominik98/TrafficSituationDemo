        },
        {
            "name": "GENERATE",
            "type": "debugpy",
            "request": "launch",
            "program": "src/generate_scenic.py",
            "console": "integratedTerminal"
        },
        {
            "name": "Scenic-Newtonian solution9",
            "type": "debugpy",
            "request": "launch",
            "module": "scenic",
            "args": ["assets/scenic/newtonian/solution9.scenic", "--simulate"],
